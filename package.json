{
  "name": "sweep-odyssey-adapter",
  "version": "0.3.3",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:development": "yarn prisma db seed ; nest start --watch ",
    "start:production": "yarn prisma db push ; yarn build ; nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:reset": "yarn prisma migrate reset",
    "migrate": "yarn prisma migrate deploy",
    "watch": "tsc-watch --onSuccess \\\"node dist/src/main.js\\\" --onFailure \\\"echo There was a problem with the build!\\\" -p ./tsconfig.json"
  },
  "dependencies": {
    "@automapper/classes": "^8.4.1",
    "@automapper/core": "^8.4.1",
    "@automapper/nestjs": "^8.4.1",
    "@nestjs/bull": "^0.5.4",
    "@nestjs/common": "8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "8.0.0",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/swagger": "5.0.0",
    "@prisma/client": "^4.0.0",
    "@qiwi/decorator-utils": "^3.2.8",
    "@rxjsx/rxjsx": "^1.0.0",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.2",
    "class-transformer": "^0.5.1",
    "colorette": "^2.0.17",
    "date-fns": "^2.29.1",
    "deterministic-object-hash": "^1.3.1",
    "global": "^4.4.0",
    "nest-winston": "^1.7.0",
    "node-html-parser": "^5.3.3",
    "pg": "^8.7.3",
    "prisma": "^4.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "russian-name-detector": "^1.0.3",
    "rxjs": "^7.5.5",
    "rxjs-etc": "^10.6.2",
    "rxjs-for-await": "^1.0.0",
    "rxjs-observed-decorator": "^1.2.2",
    "swagger-ui-express": "^4.4.0",
    "ts-pattern": "^4.0.4",
    "undici": "^5.0.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@automapper/types": "^6.3.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "class-validator": "^0.13.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express-basic-auth": "^1.2.1",
    "fs-jetpack": "^4.3.1",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "rxjs-spy": "^8.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "9.2.8",
    "ts-node": "10.4.0",
    "tsc-watch": "^5.0.3",
    "tsconfig-paths": "3.11.0",
    "typescript": "^4.7.4"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seeders/index.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
